    Private Sub CheckBox_streuner_CheckedChanged(sender As Object, e As EventArgs) Handles CheckBox_streuner.CheckedChanged
        If CheckBox_streuner.Checked = True Then
            CheckBox_streuner.ForeColor = Color.Lime
        Else
            CheckBox_streuner.ForeColor = Color.Red
        End If
